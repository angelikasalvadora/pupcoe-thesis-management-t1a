
<br>
<h4>Faculties</h4>
<button type="button" class="btn btn-outline-primary float-right" id="facultybutton"><a href="/admin/faculty/add">Add Faculty</a></button>
<br>
<br>
	<div class="container">
		<table class="table table-hover">
			<thead>
				<tr>
					<th class="text-center">
						<label>Employee ID</label>
					</th>
					<th class="text-center">
						<label>Name</label>
					</th>
					<th class="text-center">
						<label>Email</label>
					</th>
					<th class="text-center">
						<label>Phone</label>
					</th>
					<th class="text-center">
						<label>Admin</label>
					</th>
					<th class="text-center">
						<label>User Type</label>
					</th>
					<th class="text-center">
						<label>Details</label>
					</th>

				</tr>
			</thead>
			<tbody>
						{{#each facultylist}}
					<tr>
						<td class="text-center">{{this.employee_id}}</td>
						<td class="text-center">{{this.firstname}} {{this.lastname}}</td>
						<td class="text-center">{{this.email}}</td>
						<td class="text-center">{{this.phone}}</td>
							<td class="text-center">{{this.adminstat}}</td>
							<td class="text-center">{{this.usertype}}</td>

						<td class="text-center"><a href="url">View</a></td>

					</tr>
	{{/each}}
			</tbody>
		</table>
	</div>
  <br><br>
