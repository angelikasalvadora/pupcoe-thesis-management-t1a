
<br>
<h4>Students</h4>
<button type="button" class="btn btn-outline-primary float-right" id="facultybutton"><a href="/admin/students/add">Add Student</a></button>
<br>
<br>
	<div class="container">

		<table class="table table-hover">
			<thead>
				<tr>
					<th class="text-center">
						<label>Student Number</label>
					</th>
					<th class="text-center">
						<label>Name</label>
					</th>
					<th class="text-center">
						<label>Email</label>
					</th>
					<th class="text-center">
						<label>Phone</label>
					</th>

					<th class="text-center">
						<label>Details</label>
					</th>

				</tr>
			</thead>
			<tbody>
				{{#each studentlist}}
					<tr>
						<td class="text-center">{{this.student_number}}</td>
						<td class="text-center">{{this.firstname}} {{this.lastname}}</td>
						<td class="text-center">{{this.email}}</td>
						<td class="text-center">{{this.phone}}</td>

						<td class="text-center"><a href="url">View</a></td>

					</tr>
	{{/each}}
			</tbody>
		</table>
	</div>
  <br><br>
